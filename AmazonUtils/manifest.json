{
    "manifest_version": 3,
    "name": "Amazon Utils.",
    "version": "2.0",
    "description": "非表示系",
    "content_scripts": [
        {
            "matches": [
                "https://www.amazon.co.jp/*"
            ],
            "js": [
                "js/pageNavigation.js"
            ]
        },
        {
            "matches": [
                "https://www.amazon.co.jp/s?k=*",
                "https://www.amazon.co.jp/hz/wishlist/ls/*",
                "https://www.amazon.co.jp/gp/bestsellers/*"
            ],
            "js": [
                "js/reviewColorizer.js"
            ]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}